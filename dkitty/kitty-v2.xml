<mujoco model="dkitty-v2">
  <include file="../scenes/basic_scene.xml"/>
  <include file="assets/dependencies_xh-v2.xml"/>
  <compiler meshdir="."/>
  <worldbody>
    <body name="kitty_frame" pos="0 0 .5" euler="0 3.14 -1.57">
      <geom type="sphere" size="0.010" mass="0.10"/>
      <camera name="trackingZ" pos="0 0 -3" zaxis="0 0 -3" mode="trackcom"/>
      <camera name="trackingY" pos="1.5 -1.5 0" xyaxes="-.7 -1 0 0 -.2 -1" mode="trackcom"/>
      <joint name="Tx" pos="0 0 0" axis="0 -1 0" limited="true" type="slide" range="-5 50"/>
      <joint name="Ty" pos="0 0 0" axis="-1 0 0" limited="true" type="slide" range="-5 5"/>
      <joint name="Tz" pos="0 0 0" axis="0 0 -1" limited="true" type="slide" range="-2 2"/>
      <joint name="Rx" pos="0 0 0" axis="0 -1 0" limited="true" range="-1.57 1.57"/>
      <joint name="Ry" pos="0 0 0" axis="-1 0 0" limited="true" range="-1.57 1.57"/>
      <joint name="Rz" pos="0 0 0" axis="0 0 -1" limited="true" range="-1.57 1.57"/>
      <include file="assets/chain_xh-v2.xml"/>
    </body>
  </worldbody>
</mujoco>
